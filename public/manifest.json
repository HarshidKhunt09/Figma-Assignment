{
  "manifest_version": 2,
  "name": "Figma-Assignment",
  "version": "1.0",
  "description": "Figma-Assignment",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["storage"],
  "content_security_policy": {
    "extension_pages": "index.html"
  },
  "web_accessible_resources": ["static/*"]
}
